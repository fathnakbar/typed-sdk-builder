{
  "name": "typed-sdk-builder",
  "version": "1.0.1",
  "description": "A lightweight, zero-dependency TypeScript library for creating a fully-typed, configuration-based REST API client that handles authentication failures automatically.",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YOUR_USERNAME/typed-sdk-builder.git"
  },
  "keywords": [
    "typescript",
    "sdk",
    "builder",
    "rest",
    "api",
    "fetch",
    "client",
    "typed",
    "auth",
    "token",
    "helper",
    "interceptor"
  ],
  "author": "YOUR_NAME <your.email@example.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YOUR_USERNAME/typed-sdk-builder/issues"
  },
  "homepage": "https://github.com/YOUR_USERNAME/typed-sdk-builder#readme",
  "devDependencies": {
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5"
  }
}
